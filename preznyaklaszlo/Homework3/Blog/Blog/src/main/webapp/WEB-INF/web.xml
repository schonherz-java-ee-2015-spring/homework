<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  

  <servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<display-name>IndexServlet</display-name>
  	<description></description>
  	<servlet-class>hu.schonherz.java.training.homework.blog.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NewBlog</servlet-name>
  	<display-name>NewBlog</display-name>
  	<description></description>
  	<servlet-class>hu.schonherz.java.training.homework.blog.NewBlog</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Details</servlet-name>
  	<display-name>Details</display-name>
  	<description></description>
  	<servlet-class>hu.schonherz.java.training.homework.blog.Details</servlet-class>
  </servlet>
  
   <servlet-mapping>
  	<servlet-name>IndexServlet</servlet-name>
  	<url-pattern>/IndexServlet</url-pattern>
  </servlet-mapping>
 <servlet-mapping>
  	<servlet-name>NewBlog</servlet-name>
  	<url-pattern>/NewBlog</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Details</servlet-name>
  	<url-pattern>/Details</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
		<welcome-file>IndexServlet</welcome-file>
	</welcome-file-list>
  
	
</web-app>
