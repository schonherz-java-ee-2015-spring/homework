<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BlogApplicationDetails</servlet-name>
  	<display-name>BlogApplicationDetails</display-name>
  	<servlet-class>hu.schonherz.training.servlet.BlogApplicationDetails</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BlogApplicationNewBlog</servlet-name>
  	<display-name>BlogApplicationNewBlog</display-name>
  	<servlet-class>hu.schonherz.training.servlet.BlogApplicationNewBlog</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BlogApplicationDetails</servlet-name>
  	<url-pattern>/BlogApplicationDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BlogApplicationNewBlog</servlet-name>
  	<url-pattern>/BlogApplicationNewBlog</url-pattern>
  </servlet-mapping>
</web-app>